# ==========================================
# LIVEKIT CLOUD CREDENTIALS
# Get these from: https://cloud.livekit.io/
# ==========================================
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# ==========================================
# AI MODEL KEYS
# ==========================================
# LLM Provider: "livekit", "gemini" or "openai"
LLM_PROVIDER=livekit
LIVEKIT_INFERENCE_LLM_MODEL=openai/gpt-4o-mini

# Optional provider credentials (only if using them)
GEMINI_API_KEY=
GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
GEMINI_MODEL=gemini-2.0-flash
OPENAI_API_KEY=

# LiveKit Inference STT (Hearing)
LIVEKIT_INFERENCE_STT_MODEL=deepgram/nova-3
LIVEKIT_INFERENCE_STT_LANGUAGE=multi
LIVEKIT_INFERENCE_ENDPOINTING_MS=25

# TTS Provider: "livekit", "gemini", "openai", or "cartesia"
TTS_PROVIDER=livekit
LIVEKIT_INFERENCE_TTS_MODEL=deepgram/aura-2
LIVEKIT_INFERENCE_TTS_VOICE=
LIVEKIT_INFERENCE_TTS_LANGUAGE=
CARTESIA_API_KEY=

# ==========================================
# VOBIZ SIP TRUNK CONFIGURATION
# ==========================================
VOBIZ_SIP_DOMAIN=your-sip-domain.sip.vobiz.ai
VOBIZ_USERNAME=your_vobiz_username
VOBIZ_PASSWORD=your_vobiz_password
VOBIZ_OUTBOUND_NUMBER=+911234567890
OUTBOUND_TRUNK_ID=ST_your_trunk_id
DEFAULT_TRANSFER_NUMBER=+911234567890

# Safety toggles
ENABLE_OUTBOUND_CALLS=false
AUTO_TRANSFER_ON_FAILURE=true
SILENCE_TRANSFER_SECONDS=60
